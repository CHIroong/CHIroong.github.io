    script.src = (isHTTPS ? 'https' : 'http') + '://tour.interpark.com/exporter/banner/TourMain.aspx?method=NaviBanner';

    document.getElementsByTagName('head')[0].appendChild(script);
}

function SetNaviBanner(obj) {
    // 네비게이션 여보세요, 웨딩 배너    
    CreateNaviBanner(obj, "naviBannerArea");
}

// 파라메터 붙이기 url에 
function AddParamToUrl(url, param)
{
    if (url.indexOf('?') > -1) {
        return (url + "&" + param);
    }
    else {
        return (url + "?" + param);
    }

}

// 상단 게이트 소배너
function CreateNaviBanner(obj, targetid) {
    //bannerInfo["KeywordA"] = 파라메터
    try{
        if (obj != null && obj.ListBanner.length > 0)
        {
            var html = '';

            for (var i = 0 ; i < obj.ListBanner.length; i++)
            {
                var bannerInfo = obj.ListBanner[i];

                if (bannerInfo["LinkURL"].length > 0)
                    html += '<a href="' + (bannerInfo["LinkURL"] == "" ? "#" : AddParamToUrl(bannerInfo["LinkURL"], bannerInfo["KeywordA"])) + '" target="' + (bannerInfo["IsNewWindow"] == "Y" ? "_blank" : "_self") + '">';

                html += '   <img src="http://tourimage.interpark.com/Sites/Tour/ByTemplate/030/' + bannerInfo["ImageURL"] + '" alt="' + bannerInfo["Keyword"] + '" onerror="this.style.display=\'none\';" />';            

                if (bannerInfo["LinkURL"].length > 0)
                    html += '</a>';

            }

            document.getElementById(targetid).innerHTML = replaceSSLHttps(replaceSSLUrl(html)).replace('/&', '/?');
        }
    }
    catch(e){}
}

