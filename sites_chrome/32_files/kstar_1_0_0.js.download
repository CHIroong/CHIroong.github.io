function _KstB(){
	this.version = "1.0.0";
	this.corp = "KStar";
	this.width  ="0px";
	this.height ="0px";
	this.reqtag = "";

	_KstB.instanceCount = _KstB.instanceCount ? _KstB.instanceCount + 1 : 1;

	this.show = function(){
		if(this.reqtag.trim().length!=0){
			var tag='';
			if(this.reqtag.indexOf('?') != -1){
				tag= this.reqtag+'&rnd='+Math.floor(Math.random() * 100);
			}else{
				tag= this.reqtag+'?rnd='+Math.floor(Math.random() * 100);
			}
//			document.open();
//			document.write('<div mar'+'ginwidth="0px" marginheight="0px" style="width:'+this.width+';height:'+this.height+';border:0px;overflow:hidden;display:inline-block;">');
			document.write('<div mar'+'ginwidth="0px" marginheight="0px" style="width:100%;height:'+this.height+';border:0px;overflow:hidden;display:inline-block;text-align:center">');
			document.write('<if'+'rame frameborder="0" style="width:'+this.width+';height:'+this.height+';border:0px;margin:0;overflow:hidden;" src="'+tag+'"></ifr'+'ame>');
			document.write('</div>');
//			document.close();
		}

	}
}

