$(document).ready(function() {
    event_autoRolling(Math.floor(Math.random()*1+1));
});

function change_rolledit(bnum){
	for(e=1; e<4; e++){
		document.getElementById('rolledit'+e).style.display = 'none';
	}
		document.getElementById('rolledit'+bnum).style.display = 'block';
}

function rolledit(val){
	if(val == 'n'){
		event_random = event_random + 1;
		if(event_random >3){
			event_random = 1;
		}
	}else{
		event_random = event_random - 1;
		if(event_random < 1){
			event_random =3;
		}
	}
	delay = 1; // 오토 롤링 stop
	clearInterval(temp);	
	change_rolledit(event_random);
}


function auto_rolledit(val){
	if(val == 'n'){
		event_random = event_random + 1;
		if(event_random >3){
			event_random = 1;
		}
	}else{
		event_random = event_random - 1;
		if(event_random < 1){
			event_random =3;
		}
	}

	change_rolledit(event_random);
}

 var delay = 6500; // 레이어 전환 지연시간

 var temp;


function event_autoRolling(event_random)
 {
	
  clearInterval(temp);

  temp = setInterval("auto_rolledit(event_random)", delay);
 }


