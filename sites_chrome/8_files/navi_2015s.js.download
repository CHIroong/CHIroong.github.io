  function uniqArr(arr) {
        var chk = [];
        for (var i = 0; i < arr.length; i++) {
            if (chk.length == 0) {
                chk.push(arr[i]);
            } else {
                var flg = true;
                for (var j = 0; j < chk.length; j++) {
                    if (chk[j] == arr[i]) {
                        flg = false;
                        break;
                    }
                }
                if (flg) {
                    chk.push(arr[i]);
                }
            }
        }
        return chk;
    }





