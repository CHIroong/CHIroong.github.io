	
function change_edit(bnum){
	for(e=1; e<4; e++){
		document.getElementById('edit'+e).style.display = 'none';
	}
		document.getElementById('edit'+bnum).style.display = 'block';
}

function edit(val){
	if(val == 'n'){
		event_random = event_random + 1;
		if(event_random > 3){
			event_random = 1;
		}
	}else{
		event_random = event_random - 1;
		if(event_random < 1){
			event_random = 3;
		}
	}
	change_edit(event_random);
}


