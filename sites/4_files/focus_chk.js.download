function onLoad_Chk(fv){
	vv = fv.value;
	if (vv == '') {
		fv.style.backgroundColor = '#FFFFFF';
	} else {
		fv.style.backgroundColor = '#FFFAE5';
	}
}

function onBlur_Chk(fv){
	vv = fv.value;
	if (vv == '') {
		fv.style.backgroundColor = '#FFFFFF';
	} else {
		fv.style.backgroundColor = '#E8F6FC';
	}
}

