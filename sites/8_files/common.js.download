	addFrm.setAttribute("height",frm_height+"px");
	addFrm.setAttribute("border","0");
	addFrm.setAttribute("frameBorder","0");
	addFrm.setAttribute("marginWidth","0");
	addFrm.setAttribute("marginHeight","0");
	addFrm.setAttribute("leftMargin","0");
	addFrm.setAttribute("topMargin","0");
	addFrm.setAttribute("allowtransparency","true");
	addFrm.setAttribute("scrolling","no");
	addFrm.setAttribute("src",frm_url);
	eval("document.getElementById('"+div_name+"')").appendChild(addFrm);
}



function LEFT_mm_PHOTOScroll()
{
	
	
	this._hGAP=0;
	this._hINIT=0;
	this._hBTTM=0;
	this._hADHT=0;
	this._hMaxSCHT=0;
	this._hRefreshVal=120;
	this._hScrollSpeed=2;
	this._hTimer;
	this._FLayer;
	this._SLayer;
	this.name="LEFT_mm_PHOTOScroll";
	
	this.AddLayer=function(FLayer,SLayer)
	{
		this._FLayer=document.getElementById(FLayer);
		this._SLayer=document.getElementById(SLayer);
	};
	this.RefreshScrollPhoto=function()
	{
		var _hStartPoint,_hEndPoint,_hRefreshTime,_hScrollAmount;
		this._hINIT=this._FLayer.offsetTop+this._hGAP;
		this._hMaxSCHT=document.body.scrollHeight;
		_hStartPoint=parseInt(this._SLayer.style.top,10);
		if(typeof(window.pageYOffset)=="number")
		{
			_hEndPoint=window.pageYOffset+this._hGAP;
		}else if(typeof(document.documentElement.scrollTop)=="number"){
			_hEndPoint=document.documentElement.scrollTop+this._hGAP;
		}else{
			_hEndPoint=0;
		}
		if(_hEndPoint<this._hINIT)_hEndPoint=this._hINIT;
		if(_hEndPoint>(this._hMaxSCHT-this._hADHT-this._hBTTM))_hEndPoint=this._hMaxSCHT-this._hADHT-this._hBTTM;
		_hRefreshTime=this._hRefreshVal;
		if(_hStartPoint!=_hEndPoint&&_hEndPoint<=(this._hMaxSCHT-this._hADHT-this._hBTTM)&&_hEndPoint>=this._hINIT)
		{
			this._hScrollAmount=Math.ceil(Math.abs(_hEndPoint-_hStartPoint)/15);
			this._SLayer.style.top=_hStartPoint+((_hEndPoint<_hStartPoint)?-this._hScrollAmount:this._hScrollAmount)+"px";
			_hRefreshTime=this._hScrollSpeed;
		}
		this._SLayer.style.left=this._FLayer.offsetLeft+"px";
		this._hTimer=setTimeout(this.name+".RefreshScrollPhoto()",_hRefreshTime);
		};
		this.InitScrollPhoto=function(_hADHT,_hBTTM)
		{
			this._hADHT=_hADHT;
			this._hBTTM=_hBTTM;
			this._hINIT=this._FLayer.offsetTop+this._hGAP;
			this._SLayer.style.top=this._hINIT+"px";
			this._hMaxSCHT=document.body.scrollHeight;
			this.RefreshScrollPhoto();
		};
}

