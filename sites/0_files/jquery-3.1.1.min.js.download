		if(refer!=undefined && refer!="" && refer.indexOf("img.sbs.co.kr") == -1){

			setInterval(function() {
				secCount--;
				ga('send', 'event', 'countdown',  'page_error', secCount);
				ga('clientTracker.send', 'event', 'countdown',  'page_error', secCount);

				if(secCount < 1)  {
					ga('send', 'event', 'move',  'page_error', refer);
					ga('clientTracker.send', 'event', 'move',  'page_error', refer);

					document.location.href= refer;return 
				}
				$(".errorCount").html("<h2>"+secCount+"</h2> <p>이전 페이지로 돌아갑니다.</p>");
			 }, 1000);
		}else{
				setInterval(function() {
				secCount--;
				ga('send', 'event', 'countdown',  'page_error', secCount);
				ga('clientTracker.send', 'event', 'countdown',  'page_error', secCount);

				if(secCount < 1)  {
					ga('send', 'event', 'move',  'page_error', "//news.sbs.co.kr");
					ga('clientTracker.send', 'event', 'move',  'page_error', "//news.sbs.co.kr");

					document.location.href="//news.sbs.co.kr";return 
				}
				$(".errorCount").html("<h2>"+secCount+"</h2>");
			 }, 1000);

		}
	}catch(e){}
  }

  Error();
