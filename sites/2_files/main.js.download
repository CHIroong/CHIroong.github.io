	this.validateNubmer = function(eleElements){
		eleElements.keypress(function(e){
			var reg = /^\d+$/;
			var reg = new RegExp(reg);
			if(window.event){
				var code = window.event.keyCode;
			}
			else{
				var code = e.charCode;
			}
			var strKey = String.fromCharCode(code);
			var boolReturn = reg.test(strKey);
			return(boolReturn);	
		});
	};
	this.validatePhoneNubmer = function(eleElements){
		var reg = /[0-9]{3,4}/;
		var reg = new RegExp(reg);
		var boolReturn = reg.test(eleElements.val());
		return(!boolReturn);
	};
	this.checkAuthPhoneNubmer = function(){
		if($('#btn_mobile_auth').attr('auth_defalut')==0){
			var strCphoneNum2 = $('#download_cphone2').val();
			var strCphoneNum3 = $('#download_cphone3').val();
			var reg = /[0-9]{3,4}/;
			var reg = new RegExp(reg);
			var boolReturnA2 = reg.test(strCphoneNum2);
			var boolReturnA3 = reg.test(strCphoneNum3);
			var reg = /1{3,4}|2{3,4}|3{3,4}|4{3,4}|5{3,4}|6{3,4}|7{3,4}|8{3,4}|9{3,4}|0{3,4}/;
			var reg = new RegExp(reg);
			var boolReturnB2 = reg.test(strCphoneNum2);
			var boolReturnB3 = reg.test(strCphoneNum3);
			var arrNumber = strCphoneNum2.split("");
			var intCheckLength2 = 0;
			for(i=1;i<arrNumber.length;i++){
				if(Number(arrNumber[i-1])+1==Number(arrNumber[i])){
					intCheckLength2++;
					if(intCheckLength2>=2){break;}
				}else{
					intCheckLength2=0;
				}
			}
			var arrNumber = strCphoneNum3.split("");
			var intCheckLength3 = 0;
			for(i=1;i<arrNumber.length;i++){
				if(Number(arrNumber[i-1])+1==Number(arrNumber[i])){
					intCheckLength3++;
					if(intCheckLength3>=2){break;}
				}else{
					intCheckLength3=0;
				}
			}		
			console.log(intCheckLength2+"=="+intCheckLength3);
			if((boolReturnA2 && boolReturnA3) && ((boolReturnB2 || boolReturnB3) || (intCheckLength2>=2 || intCheckLength3>=2))){
				if($('#btn_mobile_auth').length>0){
					$('#btn_mobile_auth').show();
				}
			}else{
				if($('#btn_mobile_auth').length>0 && $('#country_num').val()=='82'){
					$('#btn_mobile_auth').hide();
				}			
			}
			return(true);
		}else{
			return(true);
		};
	};
