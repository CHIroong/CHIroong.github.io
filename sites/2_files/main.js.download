function halert(strTitle,contents,alertNo){
	var strClassBtn = 'btn btn-warning';
	var strWidth = objCommon.applyModalSize(530);
	switch(alertNo){
		case(1)://success
			strTitle = "<div id='halert_title1'><b><h4>"+strTitle+"</span></h4></div>";
			strClassBtn = 'btn btn-warning';
			break;
		case(2)://error
			strTitle = "<div id='halert_title2'><b><h4>"+strTitle+"</span></h4></div>";
			strClassBtn = 'btn btn-warning';
			break;
		default:
			strTitle = "<div id='halert_title1'><b><h4>"+strTitle+"</span></h4></div>";
			strClassBtn = 'btn btn-warning';
			break;
	}
	$( "#halert" ).dialog({
		title:strTitle,
		autoOpen: false,
		width: strWidth,
		height: 'auto',	
		buttons: [
					{
						'text':'',
						'class':strClassBtn,
						'click':function() {
							$("#halert").dialog('close');										
						}							
					}
				]
		});
	$( "#halert" ).dialog('open');
	$( "#halert" ).html(contents);
};
function validateEmail(strEmail) { 
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(strEmail);
};
