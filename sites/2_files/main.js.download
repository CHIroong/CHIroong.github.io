function init_tail_popup(){
	if($('#tail_popup').length>0){
		$(window).scroll(function(){
			if($('#tail_popup').prop('hide')!=1){
				var eleDoc = $(document);
				var intDocHeight = eleDoc.height();
				// var intToggleLine = (intDocHeight*0.5)-$('#tail_popup').height();
				var intToggleLine = 900;
				var docScroll = eleDoc.scrollTop();		
				if(docScroll>intToggleLine){
					$('#tail_popup').show('fade');
				}else{
					$('#tail_popup').hide('fade');
				}
			}
		});
	}
}
function fitNodeBodyImage(strObjBodySelector){
	$(strObjBodySelector+' img').each(function(){
		if($(this).width()>=640){
			var intBorder = Number($(this).css('border-width').replace('px',''));
			$(this).width('100%');
			if(intBorder>0){
				$(this).width($(this).width()-(intBorder*2));
			}
			$(this).height('auto');
			$(this).css({'margin':'0 auto'});
		}else{
			if($(this).css('float')!="right" && $(this).css('float')!="left"){
				$(this).css({'margin':'0 auto'});
			}
		}
		//$(this).css({'display':'inline'});
		$(this).css({'display':'block'});
	});
};
function setImageSource(){
	$(".node_body img").each(function(){
		var strSourceText = $(this).attr('longdesc');
		var strDescText = $(this).attr('alt');
		var intFloat = $(this).css("float");
		var strImgMarginRight = (Number($(this).css('margin-right').replace('px',''))+Number($(this).css('border-width').replace('px','')))+'px';
		var strImgMarginLeft = $(this).css('margin-left');
		var intSourceTopPosition = $(this).height()-25;
		var intImageWidth = $(this).width();
		if(intImageWidth>=290 && (typeof(strSourceText)!="undefined" || typeof(strDescText)!="undefined")){
			$(this).wrap( "<div class='image_with_source_d' style='float:"+intFloat+";position: relative;overflow:hidden;'></div>" );
			if($.trim(strDescText)){
				$(this).after("<div style='margin-left:"+strImgMarginLeft+";font-size:11px;color:#666;'>"+strDescText+"</div>" );
			}
			if($.trim(strSourceText)){
				$(this).after("<div style='position: absolute;right:"+strImgMarginRight+" !important;top:"+intSourceTopPosition+"px !important;font-size:11px;color:#666;background-color:rgba(255,255,255,0.8);padding-left:5px;padding-right:5px;'>"+strSourceText+"</div>" );
			}
		}
	});
	$('.image_with_source_d').each(function(){
		$(this).next('br').remove();
	});
};
function floting(strElementID){
	  var $doc           = $(document);
	  var position       = 0;
	  var top = $doc.scrollTop(); //  
	  var screenSize     = 0;        // 
	  var halfScreenSize = 475;    //  

	  /*   */
	  var pageWidth      = 0; //  , :px
	  var leftOffet      = 961;  //  ( -,  +), :px
	  var leftMargin     = 961; //     , :px, leftOffet pageWidth   .
	  var speed          = 700;     //   : "slow", "normal", or "fast" or numeric(:msec)
	  var easing         = 'swing'; //     linear, swing
	  var $layer         = $('#'+strElementID); //  
	  var layerTopOffset = 0;   //   , :px
	  $layer.css('z-index', 10);   //  z-
	  /*   */

	  //    
	  var resetXPosition = function()
	  {
	    $screenSize = $('body').width();// 
	    halfScreenSize = $screenSize/2;//  
	    // xPosition = halfScreenSize + leftOffet;
	    xPosition = leftOffet;
	    if ($screenSize < pageWidth){
	      xPosition = leftMargin;
	    }
	    $layer.css('left', xPosition);
	    $layer.css('visibility', 'visible');
	  };

	  //        
	  if (top > 0 ){
	    $doc.scrollTop(layerTopOffset+top);
	  }else{
	    $doc.scrollTop(0);
	  }
	  //     
	  $layer.css('top',layerTopOffset);
	  var maxScroll = $(document).height();
	  //    
	  $(window).resize(this.resetXPosition);
	  
	  // 
	  var intTopMargin = 460;
	  var positionTopMargin = -470;
	  $(window).scroll(function(){
		  
		  var docScroll = $doc.scrollTop() + eval($($layer).height()) + intTopMargin;
		  if(maxScroll-docScroll>0){
			  if($doc.scrollTop()>intTopMargin){
			      layerTopOffset = positionTopMargin; 
			  }else{
			      layerTopOffset = 0 - $doc.scrollTop(); 
			  }
			  yPosition = $doc.scrollTop()+layerTopOffset;
			  if($doc.scrollTop()>intTopMargin|| yPosition > layerTopOffset){
				  $layer.animate({"top":yPosition }, {duration:speed, easing:easing, queue:false});    
			  }
		  }
		  
	  });
};
